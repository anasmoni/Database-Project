<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="20.0" prefWidth="20.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HDBMS.ManageNurse">
   <children>
      <TextField fx:id="nid" layoutX="59.0" layoutY="78.0" promptText="Nurse_ID" />
      <Label layoutX="68.0" layoutY="299.0" prefHeight="31.0" prefWidth="282.0" text="N.B: Last two fields are required only for Assign." />
      <Button fx:id="delete" layoutX="552.0" layoutY="297.0" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
      <Button fx:id="assign" layoutX="620.0" layoutY="297.0" mnemonicParsing="false" onAction="#assignAction" text="Assign" />
      <TextField fx:id="shift" layoutX="59.0" layoutY="191.0" promptText="Shift" />
      <Button fx:id="details" layoutX="484.0" layoutY="297.0" mnemonicParsing="false" onAction="#detailsAction" text="Details" />
      <TextField fx:id="room" layoutX="59.0" layoutY="135.0" promptText="Room_No" />
      <Label layoutX="237.0" prefHeight="17.0" prefWidth="309.0" text="Manage Nurses">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="isassigned" layoutX="398.0" layoutY="297.0" mnemonicParsing="false" onAction="#isassignedAction" text="IsAssigned" />
      <Button fx:id="reset" layoutX="59.0" layoutY="245.0" mnemonicParsing="false" onAction="#resetAction" text="Reset" />
      <AnchorPane prefHeight="200.0" prefWidth="200.0" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" top="5.0" />
   </padding>

   

</AnchorPane>
